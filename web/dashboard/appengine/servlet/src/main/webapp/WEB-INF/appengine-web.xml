<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>s~google.com:android-vts-staging</application>
    <!-- switch to below for production push
         CAUTION only VTS release engineer shall do such production push
         after running all of its tests and verifying the UI.
    <application>s~google.com:android-vts-internal</application>
    -->
    <version>1</version>
    <threadsafe>true</threadsafe>
    <vm>true</vm>
    <sessions-enabled>true</sessions-enabled>

    <env-variables>
      <env-var name="BIGTABLE_PROJECT" value="${bigtable.projectID}" />
      <env-var name="BIGTABLE_INSTANCE" value="${bigtable.instanceID}" />
      <env-var name="EMAIL_DOMAIN" value="${appengine.emailDomain}" />
      <env-var name="SENDER_EMAIL" value="${appengine.senderEmail}" />
      <env-var name="DEFAULT_EMAIL" value="${appengine.defaultEmail}" />
      <env-var name="SERVICE_CLIENT_ID" value="${appengine.serviceClientID}" />
      <env-var name="CLIENT_ID" value="${appengine.clientID}" />
      <env-var name="GERRIT_URI" value="${gerrit.uri}" />
      <env-var name="GERRIT_SCOPE" value="${gerrit.scope}" />
    </env-variables>

    <system-properties>
      <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
      <property name="log4j.debug" value="true" />
    </system-properties>

    <beta-settings>
      <setting name="java_quickstart" value="true" />
    </beta-settings>

</appengine-web-app>
